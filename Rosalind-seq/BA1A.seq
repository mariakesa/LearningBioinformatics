from bio import *

dna = s''
kmer = k'CCTCAAGCC'
stride = 1

def n_kmer(dna, kmer, stride) -> int:
    kmer_counter = 0
    #Have to specify this statically
    # Loop through k-mers of length km with the specified stride
    for subseq in dna.kmers(stride, k=9):
        if subseq == kmer:
            kmer_counter += 1
            
    return kmer_counter


n_kmer(dna, kmer, stride) |> print